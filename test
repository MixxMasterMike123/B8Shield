    document.addEventListener('DOMContentLoaded', function() {
        const video = document.getElementById('myVideo');
        const textBlock1 = document.getElementById('textBlock1');
        const textBlock2 = document.getElementById('textBlock2');
        const textBlock3 = document.getElementById('textBlock3');

        console.log('Video element:', video);
        console.log('Text Block 1:', textBlock1);
        console.log('Text Block 2:', textBlock2);
        console.log('Text Block 3:', textBlock3);

        if (video && textBlock1 && textBlock2 && textBlock3) {
            video.addEventListener('timeupdate', function() {
                const currentTime = video.currentTime;

                if (currentTime >= 0 && currentTime < 5) {
                    textBlock1.style.display = 'block';
                } else {
                    textBlock1.style.display = 'none';
                }

                if (currentTime >= 5 && currentTime < 10) {
                    textBlock2.style.display = 'block';
                } else {
                    textBlock2.style.display = 'none';
                }

                if (currentTime >= 10 && currentTime < 15) {
                    textBlock3.style.display = 'block';
                } else {
                    textBlock3.style.display = 'none';
                }
            });
        } else {
            console.error('One or more elements are missing:', { video, textBlock1, textBlock2, textBlock3 });
        }
    });
